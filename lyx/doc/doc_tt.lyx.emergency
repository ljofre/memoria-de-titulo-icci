\begin_layout Standard
\begin_inset Formula 
\begin{equation}
\varepsilon_{\mbox{normal}}=\lim_{\Delta s\to0}\left(\frac{\Delta s'-\Delta s}{\Delta s}\right)\label{eq:normal_strains}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
busqueda de una condición inicial en el tiempo y en el espacio
\end_layout

\begin_layout Standard
Sea 
\begin_inset Formula $\mathbf{u}_{k}\left(t\right)$
\end_inset

 el campo de desplazamiento en la posición 
\begin_inset Formula $k$
\end_inset

, por lo que la posición 
\begin_inset Formula $\mathbf{x}_{k}\left(t\right)=\mathbf{x}_{0,k}+\mathbf{u}_{k}\left(t\right)$
\end_inset

 
\end_layout

\begin_layout Standard
Sea 
\begin_inset Formula $\mathbf{x}_{\mbox{ori}}$
\end_inset

 el punto en el espacio donde se estima que ocurrió el sismo, el vector
 director de propagación radial del sismo desde el punto 
\begin_inset Formula $\mathbf{x}_{\mbox{ori}}$
\end_inset

 a 
\begin_inset Formula $\mathbf{x}_{0,k}$
\end_inset

 está dado por 
\begin_inset Formula $\frac{\mathbf{x}_{0,k}-\mathbf{x}_{\mbox{ori}}}{\left\Vert \mathbf{x}_{0,k}-\mathbf{x}_{\mbox{ori}}\right\Vert }$
\end_inset

, buscamos proyectar el movimiento sobre ese vector 
\begin_inset Formula $\frac{\mathbf{x}_{0,k}-\mathbf{x}_{\mbox{ori}}}{\left\Vert \mathbf{x}_{0,k}-\mathbf{x}_{\mbox{ori}}\right\Vert }\cdot\left(\mathbf{x}_{0,k}+\mathbf{u}_{k}\left(t\right)\right)$
\end_inset

 y hacer que esta norma sea máxima para todos los instantes 
\begin_inset Formula 
\[
\int_{\mathbb{R}}\left\Vert \left(\mathbf{x}_{0,k}-\mathbf{x}_{\mbox{ori}}\right)\cdot\left(\mathbf{x}_{0,k}+\mathbf{u}_{k}\left(t\right)\right)\right\Vert ^{2}dt
\]

\end_inset


\end_layout

\begin_layout Standard
Si tratamos de minimizar esta función a todos los sismogramas simultaneamente
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
J\left(\mathbf{x}_{\mbox{ori}}\right)=\sum_{k=1}^{N}\int_{\mathbb{R}}\left\Vert \left(\mathbf{x}_{0,k}-\mathbf{x}_{\mbox{ori}}\right)\cdot\left(\mathbf{x}_{0,k}+\mathbf{u}_{k}\left(t\right)\right)\right\Vert ^{2}dt
\]

\end_inset


\end_layout

\begin_layout Standard
el minimizador de esta función de costo coincide con la solución del siguiente
 sistema lineal por mínimos cuadrados.
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{eqnarray*}
J\left(\mathbf{x}_{\mbox{ori}}\right) & = & \sum_{k=1}^{N}\int_{\mathbb{R}}\left(\mathbf{x}_{0,k.1}-\mathbf{x}_{\mbox{ori},1}\right)^{2}\cdot\left(\mathbf{x}_{0,k,1}+\mathbf{u}_{k,1}\left(t\right)\right)^{2}+\left(\mathbf{x}_{0,k,2}-\mathbf{x}_{\mbox{ori},2}\right)^{2}\cdot\left(\mathbf{x}_{0,k,2}+\mathbf{u}_{k,2}\left(t\right)\right)^{2}+\left(\mathbf{x}_{0,k,3}-\mathbf{x}_{\mbox{ori},3}\right)^{2}\cdot\left(\mathbf{x}_{0,k,3}+\mathbf{u}_{k,3}\left(t\right)\right)^{2}dt\\
 & = & \sum_{k=1}^{N}\left(\mathbf{x}_{0,k.1}-\mathbf{x}_{\mbox{ori},1}\right)^{2}\cdot\int_{\mathbb{R}}\left(\mathbf{x}_{0,k,1}+\mathbf{u}_{k,1}\left(t\right)\right)^{2}dt+\left(\mathbf{x}_{0,k,2}-\mathbf{x}_{\mbox{ori},2}\right)^{2}\cdot\int_{\mathbb{R}}\left(\mathbf{x}_{0,k,2}+\mathbf{u}_{k,2}\left(t\right)\right)^{2}dt+\left(\mathbf{x}_{0,k,3}-\mathbf{x}_{\mbox{ori},3}\right)^{2}\cdot\int_{\mathbb{R}}\left(\mathbf{x}_{0,k,3}+\mathbf{u}_{k,3}\left(t\right)\right)^{2}dt
\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard
derivando con respecto 
\begin_inset Formula $\mathbf{x}_{\mbox{ori},1}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\partial}{\partial\mathbf{x}_{\mbox{ori},1}}J=-2\sum_{k=1}^{N}\left(\mathbf{x}_{0,k.1}-\mathbf{x}_{\mbox{ori},1}\right)\cdot\int_{\mathbb{R}}\left(\mathbf{x}_{0,k,1}+\mathbf{u}_{k,1}\left(t\right)\right)^{2}dt=0
\]

\end_inset


\end_layout

\begin_layout Standard
despejando 
\begin_inset Formula $\mathbf{x}_{\mbox{ori},1}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\mathbf{x}_{\mbox{ori},1}=\frac{\sum_{k=1}^{N}\mathbf{x}_{0,k.1}\int_{\mathbb{R}}\left(\mathbf{x}_{0,k,1}+\mathbf{u}_{k,1}\left(t\right)\right)^{2}dt}{\sum_{k=1}^{N}\int_{\mathbb{R}}\left(\mathbf{x}_{0,k,1}+\mathbf{u}_{k,1}\left(t\right)\right)^{2}dt}
\]

\end_inset


\end_layout

